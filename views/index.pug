      - #HEADER
    
header.header.active(data-header)
  .container
    a.logo(href="/")
      //- img(src="../images/logo1.png", width="11", height="53", alt="unigine home")
    nav.navbar(data-navbar)
      ul.navbar-list
        li.navbar-item
          a.navbar-link(href="/", data-nav-link) home
        li.navbar-item
          a.navbar-link(href="/packages", data-nav-link) packages
        li.navbar-item
          a.navbar-link(href="/contact", data-nav-link) contact
    if user
      li.navbar-item
        a.navbar-link(href=`/transaction/${user.id}`, data-nav-link) Transaction
      a.navbar-link(href=`/notification/${user.id}`, data-nav-link) 
        img(src="../images/noti.png", alt="notification", width="30", height="53")
      div3
        p
          | #{user.wallet} DZ
          //- img(src="../images/dollar1.png", alt="money", width="30", height="53")
      li.navbar-item
        a.navbar-link(href=`/profile/${user.id}`, data-nav-link) profile
      a.btn(href="/logout", data-btn) logout 
    else 
      //- a.btn(href="/register", data-btn) register or login
      li.navbar-item.navbar-dropdown
        a.navbar-link Account
        ul.dropdown-menu
          li
            a(href="/login") Login
          li
            a(href="/register") Register
    button.nav-toggle-btn(aria-label="toggle menu", data-nav-toggler)
      span.line.line-1
      span.line.line-2
      span.line.line-3
main
  article
    //
       
              - #HERO
            
    .hero.has-before#home
      .container
        p.section-subtitle Enjoy The Games
        h1.h1.title.hero-title
          | Free Fire's Ultimate Packages for
          br
          | True Gamers!
        if user
          a.btn(href="/packages", data-btn) see packages
        else
          a.btn(href="/register", data-btn) register now
        .hero-banner
          img.w-100(src="../images/hero-banner1.png", width="850", height="414", alt="hero banner")
          img.hero-banner-bg(src="../images/hero-banner-bg.png", alt)
    //
       
              - #UPCOMING
    section.section.upcoming#packages(aria-labelledby="upcoming-label")
      .container
        p.section-subtitle#upcoming-label(data-reveal="bottom").
          
          Upcoming packages
        h2.h2.section-title(data-reveal="bottom")
          | packages
          br
          | pay 
          span.span now
        p.section-text(data-reveal="bottom") Exciting New Packages Heading to Free Fire - Get Ready for the Ultimate Gaming Experience!"
    .column
      .wrapper
        each package in packages
          .notifications__item
            .notifications__item__avatar
              img(src="./images/freefire.jpg")
            .notifications__item__content
              span.notifications__item__title= package.name
              span.notifications__item__message= package.description
            div
              p= ` ${package.price} DZ`
              button.btn(type="button") buy
      
              
